{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "quadlet",
      "camelcase": "Quadlet",
      "title": "Podman Quadlet",
      "scope": "source.quadlet",
      "file-types": [
        "quadlet"
      ],
      "injection-regex": "^quadlet$",
      "class-name": "TreeSitterQuadlet"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Grammar for Podman Quadlet",
    "authors": [
      {
        "name": "atchman",
        "email": "atchman@hotmail.de",
        "url": "https://github.com/atchman"
      }
    ],
    "links": {
      "repository": "https://github.com/atchman/tree-sitter-quadlet"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
